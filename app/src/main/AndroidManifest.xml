<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/music"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".jazzmusic"
            android:label="Jazz Music" />
        <activity
            android:name=".Classic"
            android:label="Classic Music" />
        <activity android:name=".FsmousClassic" />
        <activity android:name=".FamousJazz" />
        <activity android:name=".FamousRock" />
        <activity android:name=".RockMusic"></activity>
    </application>

</manifest>